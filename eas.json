{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "disabled": true
      }
    },
    "standalone": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "disabled": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "disabled": true
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
